.macro exitFn
    mov eax, 1
    int 0x80

.endm

.macro display add, len
    lea ecx, \add
    mov edx, \len
    mov eax, 4
    mov ebx, 1
    int 0x80

.endm

.macro read add, len
    lea ecx, \add
    mov edx, \len
    mov eax, 3
    mov ebx, 2
    int 0x80

.endm

newline:
    .ascii "\n"
    .equ nln, .-newline
